{
  "version": "0.9.24",
  "name": "@tosnetwork/sdk",
  "description": "TOS Network software development kit for JS",
  "main": "./dist/cjs/daemon/rpc.js",
  "module": "./dist/esm/daemon/rpc.js",
  "types": "./dist/types/daemon/rpc.d.ts",
  "exports": {
    "./react/daemon": {
      "types": "./dist/types/react/daemon.d.ts",
      "require": "./dist/cjs/react/daemon.js",
      "import": "./dist/esm/react/daemon.js",
      "default": "./dist/esm/react/daemon.js"
    },
    "./daemon/websocket": {
      "types": "./dist/types/daemon/websocket.d.ts",
      "require": "./dist/cjs/daemon/websocket.js",
      "import": "./dist/esm/daemon/websocket.js",
      "default": "./dist/esm/daemon/websocket.js"
    },
    "./daemon/types": {
      "types": "./dist/types/daemon/types.d.ts",
      "require": "./dist/cjs/daemon/types.js",
      "import": "./dist/esm/daemon/types.js",
      "default": "./dist/esm/daemon/types.js"
    },
    "./daemon/rpc": {
      "types": "./dist/types/daemon/rpc.d.ts",
      "require": "./dist/cjs/daemon/rpc.js",
      "import": "./dist/esm/daemon/rpc.js",
      "default": "./dist/esm/daemon/rpc.js"
    },
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tos-network/tos-js-sdk"
  },
  "homepage": "https://github.com/tos-network/tos-js-sdk#readme",
  "license": "BSD-3-Clause",
  "scripts": {
    "test-func": "jest -t",
    "test": "jest",
    "test:ws": "node test/test_reconnection.js",
    "test:ws:server": "node test/test_server.js",
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "fix-esm": "fix-esm-import-path ./dist/esm",
    "clean": "rimraf ./dist",
    "copy-css": "copyfiles -u 1 ./src/**/*.css ./dist/cjs && copyfiles -u 1 ./src/**/*.css ./dist/esm",
    "build": "npm run clean && npm run compile && node create_esm_pkg.js && npm run fix-esm && npm run copy-css",
    "prepublishOnly": "npm run build",
    "dev-relayer": "vite ./src/xswd/relayer/test"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.23",
    "@types/ws": "^8.5.4",
    "copyfiles": "^2.4.1",
    "fix-esm-import-path": "^1.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "vite": "^7.0.5"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@noble/ed25519": "^2.0.0",
    "@noble/hashes": "^1.3.3",
    "await-to-js": "^3.0.0",
    "isomorphic-ws": "^5.0.0",
    "js-base64": "^3.7.6",
    "lossless-json": "^4.0.1",
    "qrcode": "^1.5.4",
    "ws": "^8.12.1"
  }
}
